prices = {
    "banana": 4,
    "apple": 2,
    "orange": 1.5,
    "pear": 3
}
quantity = {
    "banana": 2,
    "apple": 2,
    "orange": 2,
    "pear": 2
}
total = 0
for x in prices:
    print(x)
    print("Price:", prices[x])
    print("Quantity:", quantity[x])
    print("Money made:", prices[x]*quantity[x])
    total = total + (prices[x]*quantity[x])
print("Sum of money made :", (total))

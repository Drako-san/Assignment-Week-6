inventory = {
    'gold' : 500,
    'pouch' : ['flint', 'twine', 'gemstone'],
    'backpack' : ['xylophone','dagger', 'bedroll','bread loaf']
}
inventory['pocket'] = 'seashell', 'strange berry', 'lint'
inventory['backpack'].sort()
inventory['backpack'].remove('dagger')
inventory.update ({'gold' : 550})
print(inventory)
